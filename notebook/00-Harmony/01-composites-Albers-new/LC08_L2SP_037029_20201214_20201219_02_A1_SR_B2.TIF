<script type="text/javascript">
    $(document).ready(function()
    {
        $('#loginButton').button();

        $('.oAuthType').click(function()
        {
        	window.location = '/login' + $(this).attr('data-serviceKey');
        });

        $('#registerButton').button().click(function()
        {
        	document.location = '/register';
        });

        $('#username').focus();
    });
</script>



